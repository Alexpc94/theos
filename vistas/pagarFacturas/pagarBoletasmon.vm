function sumar(xmonto){
	var xfilas=$('#filas').val();
	
	var xres=0;
	var xmon=0;
	
	for (i=1;i<=parseInt(xfilas);i++){
		if(document.getElementById('mon_'+i).checked){
			xres=$('#montito_'+i).val();
			xmon=xmon + parseFloat(xres);
		}	
	}
	$("#xtotal").val(xmon);
}
